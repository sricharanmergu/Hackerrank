def is_leap(year):
    leap = False
    
    # Write your logic here
    1900 <= year <= 100000
    if year % 4 == 0:
        if year % 100 == 0:
            if year % 400 == 0:
                leap = True
            else:
                leap
        else:
            leap = True
    else:
        leap
                      
    
    return leap

year = int(input())
print(is_leap(year))
